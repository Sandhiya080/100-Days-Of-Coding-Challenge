public class SumCalculator {
    public static void main(String[] args) {
        int num1 = 5;
        int num2 = 7;

        int result = sumTwoNumbers(num1, num2);

        System.out.println("Sum: " + result);
    }
    public static int sumTwoNumbers(int a, int b) {
        return a + b;
    }
}
